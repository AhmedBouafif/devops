pipeline {
    agent any
    tools {
        jdk 'JDK21'
    }
    stages {
        stage('Checkout code') {
            steps {
                git branch: 'master', url: 'https://github.com/AhmedBouafif/devops.git'
            }
        }
        stage('Compile code') {
            steps {
                sh 'javac src/application/Test.java'
            }
        }
        stage('Execute code') {
            steps {
                sh 'java -cp src application.Test'
            }
        }
    }
}

